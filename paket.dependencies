group main
    source https://api.nuget.org/v3/index.json
    storage:none
    framework: netstandard2.1, net7, net8, net9
    nuget FSharp.Core
    nuget protobuf-net

group test
    source https://api.nuget.org/v3/index.json
    storage:none
    framework:net9
    nuget FSharp.Core
    nuget FsUnit
    nuget Microsoft.NET.Test.Sdk
    nuget NUnit
    nuget NUnit.Analyzers
    nuget protobuf-net
    nuget NUnit3TestAdapter